{
  "name": "hypergalaktisch/contao-animate",
  "description": "Add animate.css effects for content elements",
  "keywords": ["contao", "animate", "css-animation"],
  "type": "contao-bundle",
  "homepage": "https://github.com/hypergalaktisch/contao-animate",
  "license": "LGPL-3.0+",
  "authors": [{
    "name": "hypergalaktisch",
    "email": "info@hypergalaktisch.com",
    "homepage": "http://hypergalaktisch.com",
    "role": "Developer"
  }],
  "support": {
    "issues": "https://github.com/hypergalaktisch/contao-animate/issues"
  },
  "require": {
    "php": ">=5.6.0",
    "contao/core-bundle": "~4.3"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "conflict": {
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "Hypergalaktisch\\AnimateBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Hypergalaktisch\\AnimateBundle\\Test\\": "tests/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Hypergalaktisch\\AnimateBundle\\ContaoManager\\Plugin"
  }
}
